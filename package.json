    "@types/three": "^0.144.0",
    "@types/youtube-player": "^5.5.2",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^4.11.1",
    "@vitejs/plugin-vue": "^5.0.4",
